<Rule Name="TypeScriptCompile" DisplayName="Soubor TypeScriptu" PageTemplate="generic" Description="Soubor TypeScriptu" xmlns="http://schemas.microsoft.com/build/2009/properties">

  <Rule.Categories>
    <Category Name="General" DisplayName="Obecné"/>
  </Rule.Categories>

  <Rule.DataSource>
    <DataSource Persistence="ProjectFile" HasConfigurationCondition="false" ItemType="TypeScriptCompile"/>
  </Rule.DataSource>

  <EnumProperty Name="{}{ItemType}" DisplayName="Akce balíčku" Category="General" F1Keyword="javascript.project.property.packageaction" Description="Určuje akci pro tento soubor, když je vytvořený balíček aplikace.">
    <EnumValue Name="None" DisplayName="Žádná"/>
    <EnumValue Name="Content" DisplayName="Obsah"/>
    <EnumValue Name="AppxManifest" DisplayName="Manifest aplikace"/>
    <EnumValue Name="PRIResource" DisplayName="Prostředek"/>
    <EnumValue Name="TypeScriptCompile" DisplayName="TypeScriptCompile"/>

    <EnumProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label="Configuration" SourceType="Item" ItemType="TypeScriptCompile" HasConfigurationCondition="false"/>
    </EnumProperty.DataSource>
  </EnumProperty>

  <EnumProperty Name="CopyToOutputDirectory" DisplayName="Kopírovat do výstupního adresáře" Category="General" F1Keyword="javascript.project.property.copytooutputdirectory" Description="Určuje, jestli se má soubor zkopírovat do výstupní složky.">
    <EnumValue Name="Never" DisplayName="Nekopírovat"/>
    <EnumValue Name="Always" DisplayName="Vždycky kopírovat"/>
    <EnumValue Name="PreserveNewest" DisplayName="Kopírovat, pokud je novější"/>
  </EnumProperty>

  <StringProperty Name="Identity" DisplayName="Název souboru" ReadOnly="true" Category="General" F1Keyword="javascript.project.property.filename" Description="Název souboru nebo složky">
    <StringProperty.DataSource>
      <DataSource Persistence="Intrinsic" ItemType="TypeScriptCompile" PersistedName="Identity"/>
    </StringProperty.DataSource>
  </StringProperty>

  <StringProperty Name="FullPath" DisplayName="Úplná cesta" ReadOnly="true" Category="General" F1Keyword="javascript.project.property.fullpath" Description="Umístění souboru">
    <StringProperty.DataSource>
      <DataSource Persistence="Intrinsic" ItemType="TypeScriptCompile" PersistedName="FullPath"/>
    </StringProperty.DataSource>
  </StringProperty>

  <BoolProperty Name="Visible" Visible="false"/>
  <StringProperty Name="DependentUpon" Visible="false"/>
  <StringProperty Name="Link" Visible="false"/>
  <EnumProperty Name="SubType" Visible="false">
    <EnumValue Name="Designer"/>
  </EnumProperty>
</Rule>
