<Rule Name="ExpandedSdkNode" DisplayName="확장된 SDK 노드" PageTemplate="generic" Description="참조된 SDK의 콘텐츠 파일" PropertyPagesHidden="true" xmlns="http://schemas.microsoft.com/build/2009/properties">

  <Rule.Categories>
    <Category Name="General" DisplayName="일반"/>
  </Rule.Categories>

  <Rule.DataSource>
    <DataSource Persistence="ExpandedSdkNode" ItemType="ExpandedSdkNode"/>
  </Rule.DataSource>

  <StringProperty Name="FileName" DisplayName="파일 이름" Category="General" ReadOnly="true" F1Keyword="javascript.project.property.expandedsdknode.filename" Description="파일 또는 폴더의 이름입니다."/>

  <StringProperty Name="PackageVersion" DisplayName="응용 프로그램 패키지 버전" Category="General" ReadOnly="true" F1Keyword="javascript.project.property.expandedsdknode.packageversion" Description="응용 프로그램 패키지의 버전입니다."/>

  <StringProperty Name="FilePath" DisplayName="파일 경로" Category="General" ReadOnly="true" F1Keyword="javascript.project.property.expandedsdknode.filepath" Description="디스크의 파일 경로입니다."/>

  <StringProperty Name="URI" DisplayName="URI" Category="General" ReadOnly="true" F1Keyword="javascript.project.property.expandedsdknode.uri" Description="이 파일을 로드하는 데 사용할 수 있는 URI입니다."/>
</Rule>
