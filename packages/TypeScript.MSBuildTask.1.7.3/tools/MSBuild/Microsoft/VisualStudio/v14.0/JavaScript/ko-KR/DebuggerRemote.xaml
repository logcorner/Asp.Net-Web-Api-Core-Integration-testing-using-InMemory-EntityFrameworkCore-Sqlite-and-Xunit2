<Rule Name="AppHostRemoteDebugger" DisplayName="원격 컴퓨터" PageTemplate="debugger" Order="300" Description="원격 디버거 옵션" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns="http://schemas.microsoft.com/build/2009/properties">

  <Rule.Metadata>
    <sys:Guid x:Key="DebugTargetTypeCommandGuid">FEEA6E9D-77D8-423F-9EDE-3970CBB76125</sys:Guid>
    <sys:UInt32 x:Key="DebugTargetTypeCommandId">0x400</sys:UInt32>
  </Rule.Metadata>

  <Rule.DataSource>
    <DataSource Persistence="UserFile"/>
  </Rule.DataSource>

  <Rule.Categories>
    <Category Name="StartOptions" DisplayName="시작 옵션" Description="시작 옵션"/>
    <Category Name="RemoteOptions" DisplayName="원격 옵션" Description="원격 옵션"/>
  </Rule.Categories>

  <BoolProperty Name="LaunchApplication" DisplayName="응용 프로그램 시작" Category="StartOptions" F1Keyword="javascript.project.property.debugging.launchapplication" Description="응용 프로그램을 즉시 시작할지 아니면 실행 시작 시 응용 프로그램을 디버그하도록 대기할지를 지정합니다."/>

  <BoolProperty Name="AllowLocalNetworkLoopback" DisplayName="로컬 네트워크 루프백 허용" Category="StartOptions" F1Keyword="javascript.project.property.debugging.allowlocalnetworkloopback" Description="Windows 스토어 응용 프로그램 디버깅 시 로컬 서비스 사용, 네트워크 트래픽 디버깅 및 동일한 컴퓨터의 구성 요소와 통신하기 위한 로컬 루프백 사용을 허용합니다."/>

  <EnumProperty Name="DebuggerType" DisplayName="디버거 형식" Category="StartOptions" F1Keyword="javascript.project.property.debugging.debuggertype" Description="사용하도록 설정할 디버거를 지정합니다. [혼합]으로 설정하는 경우 관리 코드와 네이티브 코드 둘 다에 대해 디버거가 사용됩니다.">
    <EnumValue Name="NativeOnly" DisplayName="네이티브 전용" Description="네이티브 전용"/>
    <EnumValue Name="ManagedOnly" DisplayName="관리 전용" Description="관리 전용"/>
    <EnumValue Name="Script" DisplayName="스크립트만" Description="스크립트만"/>
    <EnumValue Name="Mixed" DisplayName="혼합(관리/네이티브)" Description="혼합(관리/네이티브)"/>
    <EnumProperty.Metadata>
      <NameValuePair Name="IsDebugType" Value="true"/>
    </EnumProperty.Metadata>
  </EnumProperty>

  <StringProperty Name="MachineName" DisplayName="컴퓨터 이름" Category="RemoteOptions" F1Keyword="javascript.project.property.debugging.machinename" Description="원격 디버깅 시 사용할 컴퓨터의 이름을 지정합니다.  가까운 컴퓨터를 찾으려면 드롭다운 목록을 사용하십시오.">
    <StringProperty.ValueEditors>
      <ValueEditor EditorType="JsRemoteDebuggerOptionsPropertyEditor" DisplayName="&lt;찾기...&gt;">
        <ValueEditor.Metadata>
          <NameValuePair Name="AuthenticationPropertyName" Value="RequireAuthentication"/>
        </ValueEditor.Metadata>
      </ValueEditor>
    </StringProperty.ValueEditors>
  </StringProperty>

  <EnumProperty Name="RequireAuthentication" DisplayName="인증 필요" Category="RemoteOptions" F1Keyword="javascript.project.property.debugging.requireauthentication" Description="원격 장치에서 디버깅할 때 필요한 인증 수준을 지정합니다.">
    <EnumValue Name="false" DisplayName="없음" Description="인증 안 함"/>
    <EnumValue Name="true" DisplayName="Windows" Description="Windows 인증"/>
    <EnumValue Name="Universal" DisplayName="유니버설(암호화되지 않은 프로토콜)" Description="유니버설 인증"/>
  </EnumProperty>
</Rule>
